query selectAssetByOwner{
	description: "Select all assets based on their assets"
	statement:
  		SELECT org.example.mynetwork.Commodity
  			WHERE ( owner == _$owner )
}


query selectAssetByType{
	description: "Select all assets based on their assets"
	statement:
  		SELECT org.example.mynetwork.Commodity
  			WHERE ( type == _$type )
}